<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.obolibrary.org/obo/go.owl#"
     xml:base="http://purl.obolibrary.org/obo/go.owl"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#"
     xmlns:go="http://purl.obolibrary.org/obo/go#">
    <owl:Ontology rdf:about="http://purl.obolibrary.org/obo/go.owl">

    </owl:Ontology>
    
    <!-- a tiny subset of GO that I can use for testing and for putting in breaking cases for regression -->

    <owl:AnnotationProperty rdf:about="http://purl.obolibrary.org/obo/go#goslim_plant">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Plant GO slim</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#SubsetProperty"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.obolibrary.org/obo/go#goslim_pombe -->

    <owl:AnnotationProperty rdf:about="http://purl.obolibrary.org/obo/go#goslim_pombe">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fission yeast GO slim</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#SubsetProperty"/>
    </owl:AnnotationProperty>


    <!-- and a class -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0000002">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mitochondrial genome maintenance</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007005"/>
        <oboInOwl:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GO:0000002</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The maintenance of the structure and integrity of the mitochondrial genome; includes replication and segregation of the mitochondrial chromosome.</obo:IAO_0000115>
        <oboInOwl:hasOBONamespace rdf:datatype="http://www.w3.org/2001/XMLSchema#string">biological_process</oboInOwl:hasOBONamespace>
    </owl:Class>

    <owl:Axiom>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GOC:ai</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GOC:vw</oboInOwl:hasDbXref>
        <owl:annotatedTarget rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The maintenance of the structure and integrity of the mitochondrial genome; includes replication and segregation of the mitochondrial chromosome.</owl:annotatedTarget>
        <owl:annotatedSource rdf:resource="http://purl.obolibrary.org/obo/GO_0000002"/>
        <owl:annotatedProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000115"/>
    </owl:Axiom>
    



</rdf:RDF>
